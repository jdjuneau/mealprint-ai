{
  "name":  "mealprint-ai-functions",
  "version":  "1.0.0",
  "description":  "Firebase Cloud Functions for Mealprint AI - Smart Meal Planning",
    "scripts":  {
                    "build":  "tsc || true",
                    "serve":  "npm run build \u0026\u0026 firebase emulators:start --only functions",
                    "shell":  "npm run build \u0026\u0026 firebase functions:shell",
                    "start":  "npm run shell",
                    "deploy":  "firebase deploy --only functions",
                    "logs":  "firebase functions:log"
                },
    "engines":  {
                    "node":  "18"
                },
    "main":  "index.js",
    "dependencies":  {
                         "@google-cloud/tasks":  "^5.5.2",
                         "@types/cors":  "^2.8.19",
                         "@types/express":  "^5.0.6",
                         "axios":  "^1.6.2",
                         "cors":  "^2.8.5",
                         "express":  "^5.2.1",
                         "firebase-admin":  "^12.0.0",
                         "firebase-functions":  "^4.5.0",
                         "googleapis":  "^144.0.0",
                         "openai":  "^6.14.0",
                         "stripe":  "^14.21.0"
                     },
    "devDependencies":  {
                            "@types/node":  "^20.10.0",
                            "typescript":  "^5.3.3"
                        },
    "private":  true
}
